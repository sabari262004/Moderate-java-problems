import java.util.*;
class Main {
    public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
        
    int arr[]={1,6,7,1};
     int target=6;
     List<Integer> list=new ArrayList<>();
     for(int i:arr){
         list.add(i);
     }
     boolean ans=false;
     for(int i:arr){
         if(list.contains(target-i)){
             ans=true;
             break;
         }
     }
      System.out.print(ans);
    }
}

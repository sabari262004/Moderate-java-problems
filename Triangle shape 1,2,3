
class Main {
    public static void main(String[] args) {

  
      int m=5;
     
     
      for(int i=1;i<=m;i++){
           for(int k=1;k<m+1-i;k++){
                             System.out.print(" ");
                     }
           for(int j=1;j<=i;j++){
                    
                       System.out.print(j+" ");
                       
           } System.out.println();
      }
          System.out.println();
      }
  
    
}

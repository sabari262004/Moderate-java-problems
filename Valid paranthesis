import java.util.*;
class Main {
    public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
        
      String s=sc.nextLine();
      boolean ans=true;
    while(true){
        if(s.contains("()")){
            s=s.replace("()","");
        }
        else if(s.contains("[]")){
            s=s.replace("[]","");
        }
        else if(s.contains("{}")){
            s=s.replace("{}","");
        }
        else{
            ans=s.isEmpty();
            break;
        }
    }
      System.out.print(ans);
    }
}
